{
  "opts": {
    "destination": "./doc/api",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8",
    "template": "node_modules/clean-jsdoc-theme",
    "tutorials": "./doc/tutorials"
  },
  "recurseDepth": 10,
  "source": {
    "include": ["./README.zh_CN.md", "./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  }
}
