{
  "name": "@qubit-ltd/common-app",
  "version": "1.11.17",
  "description": "A JavaScript library of utilities for developing web apps.",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-app.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-app/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-app",
  "keywords": [
    "JavaScript",
    "utility",
    "webapp"
  ],
  "main": "dist/common-app.min.cjs",
  "module": "dist/common-app.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-app.min.cjs",
      "import": "./dist/common-app.min.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run doc && npm run build:dev && npm run build",
    "test": "jest --passWithNoTests",
    "lint": "eslint ./src",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/'"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.27.1",
    "@qubit-ltd/common-ui": "^1.14.2",
    "@qubit-ltd/config": "^1.2.1",
    "@qubit-ltd/json": "^1.2.1",
    "@qubit-ltd/logging": "^1.4.9",
    "@qubit-ltd/pinia-decorator": "^3.4.1",
    "@qubit-ltd/storage": "^1.4.1",
    "@qubit-ltd/type-detect": "^2.2.0",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/clone": "^1.13.0",
    "@qubit-ltd/common-ui": "^1.14.2",
    "@qubit-ltd/common-util": "^5.4.1",
    "@qubit-ltd/common-validation-rule": "^1.5.0",
    "@qubit-ltd/config": "^1.2.1",
    "@qubit-ltd/eslint-config": "^1.3.5",
    "@qubit-ltd/jsdoc-minami": "^1.5.3",
    "@qubit-ltd/json": "^1.2.1",
    "@qubit-ltd/logging": "^1.4.9",
    "@qubit-ltd/naming-style": "^1.3.6",
    "@qubit-ltd/pinia-decorator": "^3.4.1",
    "@qubit-ltd/rollup-builder": "^1.8.10",
    "@qubit-ltd/storage": "^1.4.1",
    "@qubit-ltd/type-detect": "^2.2.0",
    "@qubit-ltd/typeinfo": "^3.3.0",
    "@qubit-ltd/vue3-class-component": "^2.4.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.9.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.2",
    "js-cookie": "^3.0.5",
    "jsdoc": "^4.0.4",
    "pinia": "^3.0.2",
    "rollup": "^4.40.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vue": "^3.5.13"
  },
  "packageManager": "yarn@4.5.0"
}
