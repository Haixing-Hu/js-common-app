{
  "name": "@qubit-ltd/common-app",
  "version": "1.11.23",
  "description": "A JavaScript library of utilities for developing web apps.",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-app.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-app/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-app",
  "keywords": [
    "JavaScript",
    "utility",
    "webapp"
  ],
  "main": "dist/common-app.min.cjs",
  "module": "dist/common-app.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-app.min.cjs",
      "import": "./dist/common-app.min.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc/api",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run doc && npm run build:dev && npm run build",
    "test": "jest --passWithNoTests",
    "lint": "eslint ./src",
    "doc": "jsdoc -c jsdoc.json && node scripts/add-mermaid-support.js",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/'",
    "deploy:all": "npm run deploy"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.28.4",
    "@qubit-ltd/common-ui": "^1.15.2",
    "@qubit-ltd/common-util": "^5.4.7",
    "@qubit-ltd/config": "^1.2.4",
    "@qubit-ltd/json": "^1.2.4",
    "@qubit-ltd/logging": "^1.4.12",
    "@qubit-ltd/pinia-decorator": "^3.4.4",
    "@qubit-ltd/storage": "^1.4.4",
    "@qubit-ltd/type-detect": "^2.2.3",
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@jest/core": "^30.1.3",
    "@qubit-ltd/clone": "^1.13.4",
    "@qubit-ltd/common-ui": "^1.15.2",
    "@qubit-ltd/common-util": "^5.4.7",
    "@qubit-ltd/common-validation-rule": "^1.6.5",
    "@qubit-ltd/config": "^1.2.4",
    "@qubit-ltd/eslint-config": "^2.0.0",
    "@qubit-ltd/jsdoc-theme": "^1.5.1",
    "@qubit-ltd/json": "^1.2.4",
    "@qubit-ltd/logging": "^1.4.12",
    "@qubit-ltd/naming-style": "^1.3.10",
    "@qubit-ltd/pinia-decorator": "^3.4.4",
    "@qubit-ltd/rollup-builder": "^1.8.12",
    "@qubit-ltd/storage": "^1.4.4",
    "@qubit-ltd/type-detect": "^2.2.3",
    "@qubit-ltd/typeinfo": "^3.3.4",
    "@qubit-ltd/vue3-class-component": "^2.4.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.12.2",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.45.1",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.18",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^6.0.0",
    "js-cookie": "^3.0.5",
    "jsdoc": "^4.0.4",
    "jsdoc-mermaid": "^1.0.0",
    "pinia": "^3.0.3",
    "rollup": "^4.50.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "yarn@4.5.0"
}
